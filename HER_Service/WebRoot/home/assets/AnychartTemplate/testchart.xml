			<data_plot_settings default_series_type="line">
				<line_series>
					<tooltip_settings enabled="true">
						<position anchor="LeftTop" />
						<font color="%Color" />
						<format>(x,y) = ({%XValue},{%YValue})</format>
					</tooltip_settings>
					<marker_settings>
						<states>
							<normal>
								<marker type="none" color="Green" />
							</normal>
						</states>
					</marker_settings>
				</line_series>
			</data_plot_settings>
